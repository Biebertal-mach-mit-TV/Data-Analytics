{
  "id": 1,
  "name": "football",
  "api": {
    "type": "request_multiple_custom",
    "use_loop_as_key": true,
    "steps_value": [
      "Spiele",
      "Tabelle"
    ],
    "requests": [
      {
        "type": "request",
        "url_pattern": "https://www.openligadb.de/api/getmatchdata/bl1"
      },
      {
        "type": "request",
        "url_pattern": "https://www.openligadb.de/api/getbltable/bl1/2019"
      }
    ]
  },
  "transform": [
    {
      "type": "add_symbol",
      "keys": [
        "_req|Spiele|0|Group|GroupName"
      ],
      "new_keys": [
        "_req|Spieltag"
      ],
      "pattern": "{_key}"
    },
    {
      "type": "transform_array",
      "array_key": "_req|Spiele",
      "transform": [
        {
          "type": "select",
          "relevant_keys": [
            "Team1|TeamName",
            "Team1|TeamId",
            "Team2|TeamName",
            "Team2|TeamId",
            "MatchIsFinished",
            "MatchDateTime",
            "MatchResults|0|PointsTeam1",
            "MatchResults|0|PointsTeam2"
          ],
          "throw_errors": false
        },
        {
          "type": "alias",
          "keys": [
            "_loop|MatchResults|0"
          ],
          "new_keys": [
            "_loop|MatchResults"
          ],
          "throw_errors": false
        },
        {
          "type": "date_weekday",
          "keys": [
            "_loop|MatchDateTime"
          ],
          "new_keys": [
            "_loop|weekday"
          ],
          "given_format": "%Y-%m-%dT%H:%M:%S"
        },
        {
          "type": "copy",
          "keys": [
            "_loop|MatchResults|PointsTeam1",
            "_loop|MatchResults|PointsTeam1"
          ],
          "new_keys": [
            "_loop|Team1|Won",
            "_loop|Team2|Lost"
          ]
        },
        {
          "type": "copy",
          "keys": [
            "_loop|MatchResults|PointsTeam2",
            "_loop|MatchResults|PointsTeam2"
          ],
          "new_keys": [
            "_loop|Team2|Won",
            "_loop|Team1|Lost"
          ]
        },
        {
          "type": "result",
          "keys": [
            "_loop|Team1",
            "_loop|Team2"
          ],
          "new_keys": [
            "_loop|Team1|Result",
            "_loop|Team2|Result"
          ],
          "compare_1": "Won",
          "compare_2": "Lost",
          "points": {
            "1": "1",
            "2": "3",
            "3": "0"
          }
        }
      ]
    },
    {
      "type": "transform_array",
      "array_key": "_req|Tabelle",
      "transform": [
        {
          "type": "select",
          "relevant_keys": [
            "TeamName",
            "TeamInfoId",
            "Points",
            "Goals",
            "OpponentGoals",
            "GoalDiff",
            "Matches"
          ]
        },
        {
          "type": "alias",
          "keys": [
            "_loop|TeamInfoId"
          ],
          "new_keys": [
            "_loop|TeamId"
          ]
        },
        {
          "type": "copy",
          "keys": [
            "_loop|Points",
            "_loop|Goals",
            "_loop|OpponentGoals",
            "_loop|GoalDiff"
          ],
          "new_keys": [
            "_loop|Points_before",
            "_loop|Goals_before",
            "_loop|OpponentGoals_before",
            "_loop|GoalDiff_before"
          ]
        }
      ]
    },
    {
      "type": "transform_compare_arrays",
      "array_key_1": "_req|Tabelle",
      "array_key_2": "_req|Spiele",
      "compare": "TeamName",
      "where": "Team1",
      "pattern": "Result",
      "new_key": "_loop|Result"
    },
    {
      "type": "transform_compare_arrays",
      "array_key_1": "_req|Tabelle",
      "array_key_2": "_req|Spiele",
      "compare": "TeamName",
      "where": "Team2",
      "pattern": "Result",
      "new_key": "_loop|Result"
    },
    {
      "type": "transform_compare_arrays",
      "array_key_1": "_req|Tabelle",
      "array_key_2": "_req|Spiele",
      "compare": "TeamName",
      "where": "Team1",
      "pattern": "Won",
      "new_key": "_loop|Goals_Won"
    },
    {
      "type": "transform_compare_arrays",
      "array_key_1": "_req|Tabelle",
      "array_key_2": "_req|Spiele",
      "compare": "TeamName",
      "where": "Team2",
      "pattern": "Won",
      "new_key": "_loop|Goals_Won"
    },
    {
      "type": "transform_compare_arrays",
      "array_key_1": "_req|Tabelle",
      "array_key_2": "_req|Spiele",
      "compare": "TeamName",
      "where": "Team1",
      "pattern": "Lost",
      "new_key": "_loop|Goals_Lost"
    },
    {
      "type": "transform_compare_arrays",
      "array_key_1": "_req|Tabelle",
      "array_key_2": "_req|Spiele",
      "compare": "TeamName",
      "where": "Team2",
      "pattern": "Lost",
      "new_key": "_loop|Goals_Lost"
    },
    {
      "type": "transform_array",
      "array_key": "_req|Tabelle",
      "transform": [
        {
          "type": "calculate",
          "action": "subtract",
          "keys": [
            "_loop|Points_before",
            "_loop|Goals_before",
            "_loop|OpponentGoals_before",
            "_loop|GoalDiff_before"
          ],
          "subtract": [
            "_loop|Result",
            "_loop|Goals_Won",
            "_loop|Goals_Lost",
            "_loop|Goals_Won"
          ]
        },
        {
          "type": "calculate",
          "action": "add",
          "keys": [
            "_loop|GoalDiff_before"
          ],
          "add": [
            "_loop|Goals_Lost"
          ]
        }
      ]
    }
  ],
