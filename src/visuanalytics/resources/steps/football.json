{
  "id": 0,
  "name": "football",
  "api": {
    "type": "request_multiple_custom",
    "use_loop_as_key": true,
    "steps_value": [
      "Spiele",
      "Tabelle"
    ],
    "requests": [
      {
        "type": "request",
        "url_pattern": "https://www.openligadb.de/api/getmatchdata/bl1"
      },
      {
        "type": "request",
        "url_pattern": "https://www.openligadb.de/api/getbltable/bl1/2019"
      }
    ]
  },
  "transform": [
    {
      "type": "append",
      "key": "_req|Spiele|0|Group|GroupName",
      "new_key": "_req|Spieltag"
    },
    {
      "type": "append",
      "key": "_req|Spiele|0|LeagueName",
      "new_key": "_req|LeagueName"
    },
    {
      "type": "transform_array",
      "array_key": "_req|Spiele",
      "transform": [
        {
          "type": "select",
          "relevant_keys": [
            "Team1|TeamName",
            "Team1|TeamId",
            "Team2|TeamName",
            "Team2|TeamId",
            "MatchResults|0|PointsTeam1",
            "MatchResults|0|PointsTeam2",
            "MatchDateTime",
            "MatchIsFinished"
          ]
        },
        {
          "type": "alias",
          "keys": [
            "_loop|MatchResults|0"
          ],
          "new_keys": [
            "_loop|MatchResults"
          ]
        },
        {
          "type": "date_format",
          "keys": [
            "_loop|MatchDateTime"
          ],
          "new_keys": [
            "_loop|Matchday"
          ],
          "given_format": "%Y-%m-%dT%H:%M:S",
          "format": "%a., %d.%m.%Y %H:%M"
        },
        {
          "type": "result",
          "keys": [
            "_loop|MatchResults|PointsTeam1",
            "_loop|MatchResults|PointsTeam2"
          ],
          "new_keys": [
            "_loop|Team1|Result",
            "_loop|Team2|Result"
          ],
          "dict": {
            "unentschieden": 1,
            "gewonnen": 3,
            "verloren": 0
          }
        }
      ]
    },
    {
      "type": "transform_array",
      "array_key": "_req|Tabelle",
      "transform": [
        {
          "type": "select",
          "relevant_keys": [
            "TeamName",
            "TeamInfoId",
            "Points",
            "Won",
            "Lost",
            "GoalDiff"
          ]
        },
        {
          "type": "alias",
          "keys": [
            "_loop|TeamInfoId"
          ],
          "new_keys": [
            "_loop|TeamId"
          ]
        },
        {
          "type": "add_data",
          "new_key": "_idx",
          "pattern": "Result"
        },
        {
          "type": "add_data",
          "new_key": "_idx",
          "pattern": "Points_before"
        },
        {
          "type": "add_data",
          "new_key": "_idx",
          "pattern": "Won_before"
        },
        {
          "type": "add_data",
          "new_key": "_idx",
          "pattern": "Lost_before"
        },
        {
          "type": "add_data",
          "new_key": "_idx",
          "pattern": "GoalDiff_before"
        }
      ]
    },
    {
      "type": "transform_array",
      "array_key": "_req",
      "transform": [
        {
          "type": "copy",
          "key": [
            "Tabelle|_idx|Points",
            "Tabelle|_idx|Won",
            "Tabelle|_idx|Lost",
            "Tabelle|_idx|GoalDiff"
          ],
          "new_key": [
            "Tabelle|_idx|Points_before",
            "Tabelle|_idx|Won_before",
            "Tabelle|_idx|Lost_before",
            "Tabelle|_idx|GoalDiff_before"
          ]
        }
      ]
    }
  ],
  "images": {
  },
